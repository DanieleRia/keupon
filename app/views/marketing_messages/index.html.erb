<h1>Listing marketing_messages</h1>

<table width="1000" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <th>Message</th>
    <th>Display</th>
  </tr>

<% @marketing_messages.each do |marketing_message| %>
  <tr>
    <td><%=h marketing_message.message %></td>
    <td><%=h marketing_message.display %></td>
    <td><%= link_to 'Edit', edit_marketing_message_path(marketing_message) %></td>
    <td><%= link_to 'Destroy', marketing_message, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New marketing_message', new_marketing_message_path %>
