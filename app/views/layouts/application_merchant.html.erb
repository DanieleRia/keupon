<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="en" xml:lang="en">
  <head>
    <meta http-equiv="Content-type" content="text/html; charset=utf-8" />
    <title>Keupons</title>
    <%= javascript_include_tag :defaults%>
    <% CalendarDateSelect.format = :italian %>
    <%= calendar_date_select_includes "red" %>
    <!-- <link href="/stylesheets/calendar/red/style.css" rel="stylesheet" type="text/css" /> -->
    <script src="/javascripts/jquery-latest.js" type="text/javascript"></script>
    <script src="/javascripts/validation.js" type="text/javascript"></script>
    <script type="text/javascript">
      var j = jQuery.noConflict();
    </script>
    <%= stylesheet_link_tag 'style_merchant' %>

    <script src="/javascripts/FusionMaps.js" type="text/javascript"></script>
    <script src="/javascripts/FusionCharts.js" type="text/javascript"></script>
    <link href="/stylesheets/charts/style.css" media="screen" rel="stylesheet" type="text/css" />
    <script type="text/javascript">
     j(function(){
	j('#webmenu li').hover(function(){
		j(this).children('ul').stop(true,true).show('slow');
	},function(){
		j(this).children('ul').stop(true,true).hide('slow');
	});

	j('#webmenu li').hover(function(){
		j(this).children('div').stop(true,true).show('slow');
	},function(){
		j(this).children('div').stop(true,true).hide('slow');
	});
});
    </script>
  </head>
  <body>
  <table width="1000" border="0" align="center" cellpadding="0" cellspacing="0">
  <tr>
    <td width="14" height="500" align="left" background="/images/merchant//left_gray_gradient.jpg"><img src="/images/merchant/left_gray_gradient.jpg" width="14" height="6" /></td>
    <td width="972" valign="top" bgcolor="#FFFFFF"><table width="100%" border="0" cellspacing="0" cellpadding="0">
      <tr>
        <td><%= render :partial => "shared/user_bar"%></td>
      </tr>

      <tr>
       <%= render :partial => "shared/merchant_header_menu"%>
      </tr>


      <%=yield%>
      <tr>
        <td>&nbsp;</td>
      </tr>
     <%= render :partial => "shared/footer"%>

    </table></td>
    <td width="14" align="right" background="/images/merchant/right_gray_gradient.jpg"><img src="/images/merchant/right_gray_gradient.jpg" width="14" height="6" /></td>
  </tr>
</table>
  </body>
</html>